{
  "name": "action-africastalking",
  "version": "1.0.0",
  "private": false,
  "description": "Send an SMS from GitHub Actions using Africastalking Programmable SMS",
  "keywords": [
    "actions",
    "sms",
    "africastalking"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphaolomi/action-africastalking.git"
  },
  "license": "MIT",
  "author": "Alpha Olomi <alphaolomi@gmail.com> (https://alphaolomi.com/)",
  "main": "lib/main.js",
  "scripts": {
    "all": "yarn run build && yarn run format && yarn run lint && yarn run package && yarn test",
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@typescript-eslint/parser": "^5.55.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.36.0",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
